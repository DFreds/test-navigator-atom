{
  "name": "test-navigator",
  "main": "./lib/test-navigator",
  "version": "0.0.0",
  "description": "Navigate between implementation and test files easily.",
  "keywords": [
    "test",
    "navigate",
    "test-navigator"
  ],
  "activationCommands": {
    "atom-text-editor": [
      "test-navigator:navigate"
    ]
  },
  "repository": "https://github.com/DFreds/test-navigator-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.2.0",
    "fast-levenshtein": "^2.0.6",
    "file-system": "^2.2.2",
    "lodash": "^4.17.4"
  },
  "configSchema": {
    "testFilePatterns": {
      "type": "array",
      "description": "Comma separated list of possible test patterns.",
      "default": [
        ".spec",
        "-spec",
        ".test",
        "-test",
        "Test",
        "Spec"
      ],
      "items": {
        "type": "string"
      }
    },
    "location": {
      "type": "string",
      "description": "Location of new pane",
      "default": "right",
      "enum": [
        "none",
        "right",
        "down"
      ]
    }
  }
}
